{
  "id":<%= @region.id %>,
  "name":"<%= @region.name %>"
  <% if [:yes,:partial].include? @cascade %>,
  "shapes":[
    <% shapes = @region.definition_for(@shape_set).shapes
    shapes.each_with_index do |shape| @shape = shape %>
    <%= render :partial => 'shapes/show.json' %><%= "," unless i == shapes.size-1 %>
    <% end %>
  ]
  <% end %>
}