{ "id":<%= @shape.id %>,
  "volume_value":"<%= @shape.volume_value %>",
  "name":"<%= @shape.name %>",
  "meshes":[
    <% @shape.meshes.each_with_index do |mesh, i| @mesh = mesh 
       @included[@mesh.id] = :no if @cascade == :partial and not @included[@mesh.id]==:redundant %>
    <%= render :partial => 'meshes/show.json' %><%= "," unless i == @shape.meshes.size-1 %>
    <% end %>
  ]
}