{
  "id":<%= @shape_set.id %>,
  "name":"<%= @shape_set.name %>",
  "radius":"<%= @shape_set.radius %>",
  "center_point":"<%= @shape_set.center_point %>"
  <% if [:yes,:partial].include? @cascade %>,
  "shapes":[
    <% @shape_set.shapes.each_with_index do |shape, i| @shape = shape %>
    <%= render :partial => 'shapes/show.json' %><%= "," unless i == @shape_set.shapes.size-1 %>
    <% end %>
  ]
  <% end %>
}